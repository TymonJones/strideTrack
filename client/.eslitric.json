{
    "parser": "@babel/eslint-parser",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": ["react", "jsx-a11y"],
    "env": {
      "browser": true,
      "es2021": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/prop-types": "off",
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  }
  